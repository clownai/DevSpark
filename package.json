{
  "name": "devspark-ide",
  "version": "1.0.0",
  "description": "DevSpark IDE - AI-integrated development environment",
  "main": "dist/backend/server.js",
  "scripts": {
    "start": "node dist/backend/server.js",
    "dev": "nodemon --watch src --ext ts,js --exec ts-node src/backend/server.ts",
    "build": "tsc && cp -r src/index.html src/styles src/scripts dist/",
    "test": "jest",
    "lint": "eslint src/**/*.{js,ts}",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "ide",
    "editor",
    "development",
    "ai",
    "git",
    "deployment"
  ],
  "author": "DevSpark Team",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "2.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@types/react": "19.1.1",
    "@types/react-dom": "19.1.2",
    "@types/react-redux": "7.1.34",
    "@types/styled-components": "5.1.34",
    "axios": "^1.6.7",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "monaco-editor": "^0.52.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-redux": "9.2.0",
    "redux": "5.0.1",
    "styled-components": "6.1.17"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.14",
    "@types/node": "^20.17.30",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "7.1.0",
    "ts-jest": "29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
